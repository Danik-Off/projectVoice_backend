{
    "name": "backend",
    "version": "0.0.2",
    "description": "Backend для ProjectVoice - голосового приложения с WebRTC",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "dev": "nodemon index.js",
        "sequelize": "npx sequelize-cli",
        "db:migrate": "npx sequelize-cli db:migrate",
        "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
        "db:migrate:status": "npx sequelize-cli db:migrate:status",
        "db:seed": "npx sequelize-cli db:seed:all",
        "db:seed:undo": "npx sequelize-cli db:seed:undo",
        "test": "jest --testEnvironment=node --detectOpenHandles --forceExit",
        "test:watch": "jest --watch --testEnvironment=node",
        "test:coverage": "jest --coverage --testEnvironment=node --detectOpenHandles --forceExit",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --write '**/*.{js,json,md}'",
        "prettier": "prettier --check '**/*.{js,json,md}'",
        "build": "echo \"Build command placeholder\"",
        "deploy": "echo \"Deploy command placeholder\"",
        "docs-gen": "node ./utils/swagger/swagger-gen.js",
        "create-admin": "node scripts/create-admin.js"
    },
    "keywords": [],
    "author": "Ovchinnikov Danila",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.1",
        "jsonwebtoken": "^9.0.2",
        "mysql2": "^3.11.3",
        "pg": "^8.13.0",
        "sequelize": "^6.37.4",
        "socket.io": "^4.8.0",
        "swagger-autogen": "^2.23.7",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "ws": "^8.18.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@stylistic/eslint-plugin": "^5.7.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.5",
        "eslint-plugin-promise": "^7.2.1",
        "globals": "^17.0.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.7",
        "prettier": "^3.8.0",
        "sequelize-cli": "^6.6.2",
        "supertest": "^6.3.3"
    }
}
